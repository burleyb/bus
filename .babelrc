{
  "presets": [
	[
		"@babel/preset-env",
		{
			"targets": {
				"node": "20"
			},
			"modules": "auto",
			"sourceType": "unambiguous",
			"useBuiltIns": "usage",
			"corejs": {
				"version": false,
				"proposals": true
			}
		}
	  ]
  ],
	"plugins": [
	["polyfill-corejs3", { "method": "usage-global", "version": "3" }],	
	  ["@babel/plugin-transform-runtime",
      {
        "corejs": false,
				"helpers": true,  // Enables runtime helper functions
        "regenerator": false // Enables async/await transformations  
      }
    ]
	 ]  
}
